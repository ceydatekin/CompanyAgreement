    <!-- Tempusdominus Bbootstrap 4 -->
    <link rel="stylesheet" href="~/CompanyAggrementWeb/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">
    <!-- Select2 -->
    <link rel="stylesheet" href="~/CompanyAggrementWeb/plugins/select2/css/select2.min.css">
