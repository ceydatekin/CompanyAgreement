                     class="brand-image img-circle elevation-3"
                     style="opacity: .8">
                <span class="brand-text font-weight-light"><small>Sektörel Eğitim Programı</small></span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user (optional) -->
                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <!-- Add icons to the links using the .nav-icon class
                             with font-awesome or any other icon font library -->


                        <li class="nav-item">
                            <a href="~/Admin/Index" class="nav-link">
                                <i class="nav-icon far fa-circle "></i>
                                <p class="text">Firma Ekle</p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="~/Admin/ListCompany" class="nav-link">
                                <i class="nav-icon far fa-circle "></i>
                                <p>Firma Listele</p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="~/Admin/AddCompanyQuota" class="nav-link">
                                <i class="nav-icon far fa-circle"></i>
                                <p>Firma Kontenjan Güncelle</p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="~/Admin/AssignmentAcademician" class="nav-link">
